{
    "compilerOptions": {
      "module":"CommonJS",
      "target": "ES2019",
      "sourceMap": true,
      "outDir": "./dist",
      "noImplicitAny": true,
      "declaration": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck":true,
      "forceConsistentCasingInFileNames":true 
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
